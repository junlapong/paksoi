{
  "name": "svelte-google-maps",
  "version": "0.2.0",
  "license": "MIT",
  "description": "Google Maps components for Svelte",
  "homepage": "https://github.com/timhall/svelte-google-maps",
  "main": "umd/svelte-google-maps.js",
  "module": "es/svelte-google-maps.js",
  "scripts": {
    "build": "yarn build:es && yarn build:umd",
    "build:es": "rollup -c",
    "build:umd": "rollup -c rollup.config.umd.js",
    "postversion": "yarn build"
  },
  "files": [
    "es",
    "umd",
    "src",
    "Map.html",
    "Marker.html",
    "Info.html",
    "Overlay.html",
    "Polyline.html"
  ],
  "devDependencies": {
    "rollup": "^0.52.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^4.0.0",
    "rollup-plugin-uglify-es": "^0.0.1",
    "svelte": "^1.51.0"
  }
}
